<script type="text/javascript">

$(function()
				{
				
				// this initialises the demo scollpanes on the page.
				$('.colScroll').jScrollPane();					
				$('.scroll-pane-arrows').jScrollPane(
					{
						showArrows: true,
						horizontalGutter: 10
					}
				);

				var isResizing;
				
				// and the body scrollpane
				var setContainerHeight = function()
				{
					// IE triggers the onResize event internally when you do the stuff in this function
					// so make sure we don't enter an infinite loop and crash the browser
					if (!isResizing) { 
						isResizing = true;
						$w = $(window);
						$c = $('#container');
						var p = (parseInt($c.css('paddingLeft')) || 0) + (parseInt($c.css('paddingRight')) || 0);
						$('body>.jScrollPaneContainer').css({'height': $w.height() + 'px', 'width': $w.width() + 'px'});
						$c.css({'height': ($w.height()-p) + 'px', 'width': ($w.width() - p) + 'px', 'overflow':'auto'});
						$c.jScrollPane();
						isResizing = false;	
					}
				}
				$(window).bind('resize', setContainerHeight);
				setContainerHeight();
				
				// it seems like you need to call this twice to get consistantly correct results cross browser...
				setContainerHeight();
				
				$(function() {
					$( "#dialog-modal" ).dialog({
						height: 140,
						modal: true,
						autoOpen: false,
						title: 'Add column'
					});
				});
				$(function() {
					$( ".filterModal" ).dialog({
						width: 800,
						resizable: false,
						modal: true,
						autoOpen: false,
						title: 'Filter content in this community'
					});
				});
				
				$('.filterFoot a').live('click', function() {	 
					$('.filterModal').dialog('close');
				});		
				
				
				
				
				$(function() {
					$('#col1a').hide();
					$('#col5').hide();
				});
				
				$(function() {
					var treeItem = $('#tree ul li ul li a').html();
					$('#tree ul li ul li a').click(function() {
					 //   $('#col1a').toggle();
						$('.ui-dialog-buttonpane.ui-widget-content.ui-helper-clearfix').prepend(
							'<p class="chosenFeed">Europe > Credit > Products > High Yield > TMT</p>'
							);
					    return false;
					  });
				});
		
				$(function() {
					$('.btn-feed').click(function() {
					    $("#dialog-modal").dialog('open')
						});
					    return false;
					  });
				$(function() {
					$('.filterLink').click(function() {
					    $(".filterModal").dialog('open')
						});
					    return false;
					  });
			
					$(function () {
						$("#tree").jstree({
							"plugins" : [ "themes", "html_data" ]
						});
					});
					
					$(function() {
						$( "#datepicker" ).datepicker();
					});


					
					$( "#dialog-modal" ).dialog({
								resizable: false,
								height:400,
								modal: true,
								buttons: {
									Add: function() {
										$( this ).dialog( "close" );
										$('#col1a').fadeIn('slow');
									}
								}
							});
							
					
					$(function() {
                        var message = new Array();
                        message[0] = '<form id="chatform2"><input type="text" value="Enter chat" id="msg2" /><input type="submit" value="Send"/></form>';
                        message[1] = '<form id="chatform2"><input type="text" value="http://" id="msg2" /><br/><textarea name="c2comments1" id="c2comments1" cols="30" rows="5">Description (optional)</textarea><br/><input type="submit" value="Send"/></form>';
                        message[2] = '<form id="chatform2"><input type="text" value="Title" id="msg2" /><textarea name="c2comments1" id="c2comments1" cols="30" rows="10">Body text</textarea><input type="submit" value="Send"/></form>';
                        message[3] = '<form id="chatform2"><input type="text" value="Title" id="msg2" /><br/><textarea name="c2comments1" id="c2comments1" cols="30" rows="5">Question</textarea><br/><input type="text" value="Answer 1" id="answ1" /><br/><input type="text" value="Answer 2" id="answ2" /><br/><input type="text" value="Answer 3" id="answ3" /><br/><input type="text" value="Answer 4" id="answ4" /><br/><input type="submit" value="Send"/></form>';
                        message[4] = '<form id="chatform2"><input type="text" value="Title" id="msg2" /><br/><input type="text" value="Date" id="msg2" /><textarea name="c2comments1" id="c2comments1" cols="30" rows="5">Description (optional)</textarea><br/><input type="submit" value="Send"/></form>';                       
                        message[5] = '<form id="chatform2"><input type="text" value="Browse" id="msg2" /><input type="submit" value="Upload"/><br/><textarea name="c2comments1" id="c2comments1" cols="30" rows="5">Description (optional)</textarea><br/><input type="submit" value="Send"/></form>';                       

                        $(function(){
                            $(".filter select").change(function()
                            {
                                var message_index = $(this).attr("selectedIndex");
                                var message_display = $(this).parent().find('.message_display');
                                $(message_display).empty();
                                $(message_display).append(message[message_index]).slideDown();
								
                            });
                        });
                    }); 
					

					
					
				

					
			});

			
		
</script>

